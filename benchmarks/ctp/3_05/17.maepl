(define (problem ctp-problem)
  (:domain ctp)
  (:objects A1 A2 A3 - agent
            W1 W2 W3 W4 W5 - waypoint)
  (:init (communication-enabled)
         (at A1 W1)
         (not-at A1 W2)
         (not-at A1 W3)
         (not-at A1 W4)
         (not-at A1 W5)
         (at A2 W1)
         (not-at A2 W2)
         (not-at A2 W3)
         (not-at A2 W4)
         (not-at A2 W5)
         (at A3 W1)
         (not-at A3 W2)
         (not-at A3 W3)
         (not-at A3 W4)
         (not-at A3 W5)
         (road W1 W2)
         (road W1 W4)
         (road W2 W4)
         (road W4 W5) (not-blocked W4 W5))
  (:world-nondesignated w0
    (not-blocked W1 W4)
    (blocked W1 W2)
    (blocked W2 W4))
  (:world-nondesignated w1
    (not-blocked W1 W4)
    (not-blocked W1 W2)
    (blocked W2 W4))
  (:world-designated w2
    (not-blocked W1 W4)
    (not-blocked W1 W2)
    (not-blocked W2 W4))
  (:world-nondesignated w3
    (blocked W1 W4)
    (not-blocked W1 W2)
    (not-blocked W2 W4))
  (:world-nondesignated w4
    (not-blocked W1 W4)
    (blocked W1 W2)
    (not-blocked W2 W4))
  (:observability none)
  (:goal (at A1 W5)))
